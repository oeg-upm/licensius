<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="xsl/xs3p.xsl"?>
<xs:schema xmlns:ms="http://w3id.org/meta-share/meta-share/" xmlns:omtd="http://w3id.org/meta-share/omtd-share/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="http://w3id.org/meta-share/omtd-share/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.01" xml:lang="en">
	<!--Last Updated: April 13, 2021-->
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/>
	<!--Definition for http://w3id.org/meta-share/omtd-share/DataFormat-->
	<xs:simpleType name="DataFormat">
		<xs:annotation>
			<xs:documentation xml:lang="en">The format of a computer file storing data</xs:documentation>
			<xs:appinfo>
				<identifier>http://w3id.org/meta-share/omtd-share/DataFormat</identifier>
				<label xml:lang="en">Data format</label>
				<alt_label xml:lang="en">Data type</alt_label>
				<alt_label xml:lang="en">File format</alt_label>
				<subclassOf>http://www.w3.org/2002/07/owl#Thing</subclassOf>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/meta-share/unspecified">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/meta-share/unspecified</identifier>
						<label xml:lang="en">unspecified</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationFormat">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any format used for annotated files</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnnotationFormat</identifier>
						<label xml:lang="en">Annotation format</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DataFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AlvisEnrichedDocumentFormat">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format for linguistic annotations of documents used for the ALVIS framework</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AlvisEnrichedDocumentFormat</identifier>
						<label xml:lang="en">ALVIS Enriched Document format</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Anafora">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Anafora</identifier>
						<label xml:lang="en">Anafora</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/BLAST">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/BLAST</identifier>
						<label xml:lang="en">BLAST</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Brat">
				<xs:annotation>
					<xs:documentation xml:lang="en">BRAT stand-off format for annotations (BRAT is a online environment for collaborative text annotation, cf. http://brat.nlplab.org/)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Brat</identifier>
						<label xml:lang="en">BRAT</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Cadixe_json">
				<xs:annotation>
					<xs:documentation xml:lang="en">AlvisAE protocol format</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Cadixe_json</identifier>
						<label xml:lang="en">Cadixe/JSON</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Json</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Chat">
				<xs:annotation>
					<xs:documentation xml:lang="en">CHAT (Codes for the Human Analysis of Transcripts) transcription format; used by CHILDES corpora</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Chat</identifier>
						<label xml:lang="en">CHAT</label>
						<alt_label xml:lang="en">Codes for the Human Analysis of Transcripts</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ConllFormat">
				<xs:annotation>
					<xs:documentation xml:lang="en">Formats used in the CoNLL Shared Tasks</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ConllFormat</identifier>
						<label xml:lang="en">CoNLL format</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TabularFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2000">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2000 format represents POS and Chunk tags. Fields in a line are separated by spaces. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2000</identifier>
						<label xml:lang="en">CoNLL-2000</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2002">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2002 format encodes named entity spans. Fields are separated by a single space. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2002</identifier>
						<label xml:lang="en">CoNLL-2002</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2003">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2004 format encodes named entity spans and chunk spans. Fields are separated by a single space. Sentences are separated by a blank new line. Named entities and chunks are encoded in the IOB1 format. I.e. a B prefix is only used if the category of the following span differs from the category of the current span.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2003</identifier>
						<label xml:lang="en">CoNLL-2003</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2006">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2006 (aka CoNLL-X) format targets dependency parsing. Columns are tab-separated. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2006</identifier>
						<label xml:lang="en">CoNLL-2006</label>
						<alt_label xml:lang="en">CoNLL-2007</alt_label>
						<alt_label xml:lang="en">CoNLL-X</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2008">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2008 format targets syntactic and semantic dependencies. Columns are tab-separated. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2008</identifier>
						<label xml:lang="en">CoNLL-2008</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2009">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2009 format targets semantic role labeling. Columns are tab-separated. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2009</identifier>
						<label xml:lang="en">CoNLL-2009</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Conll2012">
				<xs:annotation>
					<xs:documentation xml:lang="en">The CoNLL 2012 format targets semantic role labeling and coreference. Columns are tab-separated. Sentences are separated by a blank new line.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Conll2012</identifier>
						<label xml:lang="en">CoNLL-2012</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ConllU">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format used for CoNLL.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ConllU</identifier>
						<label xml:lang="en">CoNLL-U</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ConllFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Diaml">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format following Dialogue Act Markup Language (DiAML) which is defined within the ISO standard 24617-2</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Diaml</identifier>
						<label xml:lang="en">DIAML</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DkproTokenized">
				<xs:annotation>
					<xs:documentation xml:lang="en">DkPro format for tokenized files containing one sentence per line and tokens split by whitespaces.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DkproTokenized</identifier>
						<label xml:lang="en">DKPro tokenized</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Emma">
				<xs:annotation>
					<xs:documentation xml:lang="en">Data format according to the EMMA (Extensible MultiModal Annotation markup language) specifications, cf. https://www.w3.org/TR/2007/CR-emma-20071211/</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Emma</identifier>
						<label xml:lang="en">EMMA</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/FactoredTagLemFormat">
				<xs:annotation>
					<xs:documentation xml:lang="en">Factored tag lemma format</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/FactoredTagLemFormat</identifier>
						<label xml:lang="en">Factored tag lem format</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Folia">
				<xs:annotation>
					<xs:documentation xml:lang="en">FoLiA is an XML-based annotation format, suitable for the representation of linguistically annotated language resources</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Folia</identifier>
						<label xml:lang="en">FoLiA</label>
						<alt_label xml:lang="en">Format for Linguistic Annotation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Graf">
				<xs:annotation>
					<xs:documentation xml:lang="en">GrAF (Graph Annotation Format) is an extension of the Linguistic Annotation Framework (LAF)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Graf</identifier>
						<label xml:lang="en">GrAF</label>
						<alt_label xml:lang="en">Graph Annotation Format</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Html5Microdata">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format according to the specifications of HTML5 Microdata</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Html5Microdata</identifier>
						<label xml:lang="en">HTML5 Microdata</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Html</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/I2b2">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format of the I2B2 challenge</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/I2b2</identifier>
						<label xml:lang="en">I2B2</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InlineXml">
				<xs:annotation>
					<xs:documentation xml:lang="en">Inline XML file format</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/InlineXml</identifier>
						<label xml:lang="en">Inline XML</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Kaf">
				<xs:annotation>
					<xs:documentation xml:lang="en">KAF (also known as Knowledge Annotation Format) is a language neutral annotation format representing both morpho-syntactic and semantic annotation of documents through a stand-off multilayered structure</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Kaf</identifier>
						<label xml:lang="en">KAF</label>
						<alt_label xml:lang="en">Knowledge Annotation Format</alt_label>
						<alt_label xml:lang="en">KYOTO Annotation Format</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Json</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Lll">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format of the LLL challenge</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Lll</identifier>
						<label xml:lang="en">LLL</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MalletLdaTopicProportions">
				<xs:annotation>
					<xs:documentation xml:lang="en">Topic proportions in the shape [\t]\t\t...</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MalletLdaTopicProportions</identifier>
						<label xml:lang="en">Mallet LDA Topic Proportions</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MalletLdaTopicProportionsSorted">
				<xs:annotation>
					<xs:documentation xml:lang="en">Topic proportions in the shape [\t]\t\t... sorted</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MalletLdaTopicProportionsSorted</identifier>
						<label xml:lang="en">Mallet LDA Topic Proportions Sorted</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MsExcel">
				<xs:annotation>
					<xs:documentation xml:lang="en">Data format for Microsoft Excel documents</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MsExcel</identifier>
						<label xml:lang="en">MS-Excel</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DocumentFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TabularFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Naf">
				<xs:annotation>
					<xs:documentation xml:lang="en">The NAF format is linguistic annotation format designed for complex NLP pipelines. NAF combines strengths of the Linguistic Annotation Framework (LAF) as described in Ide et al. (2003) and the NLP Interchange Format (Hellman et al. 2013, NIF).</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Naf</identifier>
						<label xml:lang="en">NAF</label>
						<alt_label xml:lang="en">NLP Annotation Format</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/NegraExport">
				<xs:annotation>
					<xs:documentation xml:lang="en">Export format for annotated corpora in the NeGra project</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/NegraExport</identifier>
						<label xml:lang="en">NeGra export</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Nif">
				<xs:annotation>
					<xs:documentation xml:lang="en">The NLP Interchange Format (NIF) is an RDF/OWL-based format that aims to achieve interoperability between Natural Language Processing (NLP) tools, language resources and annotations; it consists of specifications, ontologies and software (overview), which are combined under the version identifier "NIF 2.0", but are versioned individually</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Nif</identifier>
						<label xml:lang="en">NIF</label>
						<alt_label xml:lang="en">NLP Interchage Format</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/RdfFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/CorpusFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationFormat</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/RdfFormats</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Pml">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format according to the Prague Markup Language (http://ufal.mff.cuni.cz/jazz/PML/index_en.html); PML is a generic data format based on XML intended for storing linguistically annotated data, such as the Prague Dependency Treebank, also annotation lexicons, etc.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Pml</identifier>
						<label xml:lang="en">PML</label>
						<alt_label xml:lang="en">Prague Markup Language</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
