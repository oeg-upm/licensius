						<identifier>http://w3id.org/meta-share/omtd-share/TruthLabelling</identifier>
						<label xml:lang="en">Truth labelling</label>
						<alt_label xml:lang="en">Truth detection</alt_label>
						<alt_label xml:lang="en-us">Truth labeling</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationRetrieval">
				<xs:annotation>
					<xs:documentation xml:lang="en">The activity of obtaining information resources relevant to an information need from a collection of information resources; searches can be based on full-text or other content-based indexing</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/InformationRetrieval</identifier>
						<label xml:lang="en">Information Retrieval</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CrossLingualSearch">
				<xs:annotation>
					<xs:documentation xml:lang="en">The ability of a system to retrieve relevant documents in various languages in response to a user query that is formulated in only one language</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/CrossLingualSearch</identifier>
						<label xml:lang="en">Cross-language Information Retrieval</label>
						<alt_label xml:lang="en">CLIR</alt_label>
						<alt_label xml:lang="en">Cross-lingual information retrieval</alt_label>
						<alt_label xml:lang="en">Translingual information retrieval</alt_label>
						<alt_label xml:lang="en">Cross-lingual search</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ExploratorySearch">
				<xs:annotation>
					<xs:documentation xml:lang="en">A type of search which, in contrast to traditional lookup search, covers a broad class of activities, such as investigating, evaluating, comparing, and synthesizing</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ExploratorySearch</identifier>
						<label xml:lang="en">Exploratory search</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationFiltering">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of removing (filtering out) redundant or unwanted information from an information stream using (semi)automated or computerized methods prior to presentation to a human user; the selection of the items is based on the correlation between the content of the items and the userâ€™s preferences (content-based filtering) or the correlation between people with similar preferences (collaborative filtering)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/InformationFiltering</identifier>
						<label xml:lang="en">Information filtering</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationFilteringByRecommenderSystems">
				<xs:annotation>
					<xs:documentation xml:lang="en">The delivery of information in the form of suggestions by recommender systems; recommender systems seek to predict the ""rating"" or ""preference"" that a user would give to an item</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/InformationFilteringByRecommenderSystems</identifier>
						<label xml:lang="en">Information filtering by recommender systems</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationFiltering</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MultilingualInformationRetrieval">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MultilingualInformationRetrieval</identifier>
						<label xml:lang="en">Multilingual Information Retrieval</label>
						<alt_label xml:lang="en">MIR</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MultimediaRetrieval">
				<xs:annotation>
					<xs:documentation xml:lang="en">The activity of obtaining multimedia information resources relevant to an information need from a collection of multimedia information resources</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MultimediaRetrieval</identifier>
						<label xml:lang="en">Multimedia Retrieval</label>
						<alt_label xml:lang="en">Multimedia Information Retrieval</alt_label>
						<alt_label xml:lang="en">Multimedia Mining</alt_label>
						<alt_label xml:lang="en">MR</alt_label>
						<alt_label xml:lang="en">MMIR</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticSearch">
				<xs:annotation>
					<xs:documentation xml:lang="en">A type of search that seeks to improve search accuracy by understanding the searcher's intent and the contextual meaning of terms as they appear in the searchable dataspace, whether on the Web or within a closed system, to generate more relevant results.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SemanticSearch</identifier>
						<label xml:lang="en">Semantic search</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TerminologySearch">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TerminologySearch</identifier>
						<label xml:lang="en">Terminology search</label>
						<alt_label xml:lang="en">Term search</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of extracting, organising and systematising knowledge usually of a specific domain from external sources so that it can be used in a knowledge-based system</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</identifier>
						<label xml:lang="en">Knowledge acquisition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OntologyAcquisition">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of creating an ontology based on other resources (corpora, other lexical resources, etc.)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/OntologyAcquisition</identifier>
						<label xml:lang="en">Ontology acquisition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OntologyEnhancement">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of improving an ontology, typically by adding new relations or entities</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/OntologyEnhancement</identifier>
						<label xml:lang="en">Ontology enhancement</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of automatically searching large volumes of data for patterns that can be considered knowledge about the data</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</identifier>
						<label xml:lang="en">Knowledge Discovery</label>
						<alt_label xml:lang="en">KD</alt_label>
						<alt_label xml:lang="en">Knowledge Discovery in Databases</alt_label>
						<alt_label xml:lang="en">KDD</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeRepresentation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of representing information about entities in a form that machines are capable of understanding it</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/KnowledgeRepresentation</identifier>
						<label xml:lang="en">Knowledge Representation</label>
						<alt_label xml:lang="en">KR</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Prediction">
				<xs:annotation>
					<xs:documentation xml:lang="en">In Machine Learning, it refers to the use of algorithms that learn from previous data in order to make predictions on data (by estimating probabilities from previous data)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Prediction</identifier>
						<label xml:lang="en">Prediction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextSpeechDataAnalytics">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TextSpeechDataAnalytics</identifier>
						<label xml:lang="en">Text/Speech/Data analytics</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Operation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeechAnalytics">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeechAnalytics</identifier>
						<label xml:lang="en">Speech analytics</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextSpeechDataAnalytics</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextAndDataAnalytics">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of converting unstructured text and data into high-quality structured data that can be further analysed to extract knowledge, support decision making etc.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TextAndDataAnalytics</identifier>
						<label xml:lang="en">Text and Data Analytics</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextSpeechDataAnalytics</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Translation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of expressing the sense of a sequence of words in one language into another language</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Translation</identifier>
						<label xml:lang="en">Translation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Operation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ImageTranslation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The automatic extraction of text contained in an image (e.g. by OCR technology), its subsequent translation into another language, followed by a digital image processing step in order to reconstruct the original image with the translated text [adapted from https://en.wikipedia.org/wiki/Image_translation]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ImageTranslation</identifier>
						<label xml:lang="en">Image translation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Translation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Localization">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process of adapting a product or content to a specific locale or market, including translation of relevant language material but also converting to local measures and currency units, modifications necessary to adapt to cultural differences of the target audience, etc. [adapted from https://www.gala-global.org/industry/intro-language-industry/what-localization]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Localization</identifier>
						<label xml:lang="en">Localization</label>
						<alt_label xml:lang="en-uk">Localisation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Translation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MachineTranslation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The automatic translation of a text from one language into another performed by software without human involvement</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MachineTranslation</identifier>
						<label xml:lang="en">Machine Translation</label>
						<alt_label xml:lang="en">Automated Translation</alt_label>
						<alt_label xml:lang="en">MT</alt_label>
						<alt_label xml:lang="en">Computer Translation</alt_label>
						<alt_label xml:lang="en">Automatic Translation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Translation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ComputerAidedTranslation">
				<xs:annotation>
					<xs:documentation xml:lang="en">A form of translation performed by a human translator with the aid of software programmes</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ComputerAidedTranslation</identifier>
						<label xml:lang="en">Computer-aided translation</label>
						<alt_label xml:lang="en">Computer-assisted translation</alt_label>
						<alt_label xml:lang="en">CAT</alt_label>
						<alt_label xml:lang="en">Machine-aided human translation</alt_label>
						<alt_label xml:lang="en">MAHT</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineTranslation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/HumanAidedMachineTranslation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process of performing Machine Translation facilitated by pre-editing or post-editing steps, or interactive human intervention to steer or select from alternative translations</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/HumanAidedMachineTranslation</identifier>
						<label xml:lang="en">Human Aided Machine Translation</label>
						<alt_label xml:lang="en">HAMT</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineTranslation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeechToSpeechTranslation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process by which spoken utterances in one language are automatically translated and spoken aloud in another language</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeechToSpeechTranslation</identifier>
						<label xml:lang="en">Speech-to-Speech translation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Translation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeechToTextTranslation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeechToTextTranslation</identifier>
						<label xml:lang="en">Speech-to-Text translation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Translation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--Definition for http://w3id.org/meta-share/omtd-share/TdmMethod-->
	<xs:simpleType name="TdmMethod">
		<xs:annotation>
			<xs:documentation xml:lang="en">The method used by a TDM/LT algorithm</xs:documentation>
			<xs:appinfo>
				<identifier>http://w3id.org/meta-share/omtd-share/TdmMethod</identifier>
				<label xml:lang="en">TDM/LT Method</label>
				<subclassOf>http://www.w3.org/2002/07/owl#Thing</subclassOf>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Crowdsourcing">
				<xs:annotation>
					<xs:documentation xml:lang="en">The practice of obtaining needed services, ideas, or content by soliciting contributions from a large group of people and especially from the online community rather than from traditional employees or suppliers</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Crowdsourcing</identifier>
						<label xml:lang="en">Crowdsourcing</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/FiniteStateTechnology">
				<xs:annotation>
					<xs:documentation xml:lang="en">In data communications, flow control is the process of managing the rate of data transmission between two nodes to prevent a fast sender from overwhelming a slow receiver. It provides a mechanism for the receiver to control the transmission speed, so that the receiving node is not overwhelmed with data from transmitting node.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/FiniteStateTechnology</identifier>
						<label xml:lang="en">Finite state technology</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">Methods and techniques used either in machine learning or statistical learning</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</identifier>
						<label xml:lang="en">Machine and Statistical Learning Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ArtificialNeuralNetwork">
				<xs:annotation>
					<xs:documentation xml:lang="en">A computational model based on a large collection of simple neural units (artificial neurons), loosely analogous to the observed behavior of a biological brain's axons. These systems are self-learning and trained, rather than explicitly programmed.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ArtificialNeuralNetwork</identifier>
						<label xml:lang="en">Artificial Neural Network</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AssociationRuleLearning">
				<xs:annotation>
					<xs:documentation xml:lang="en">A machine learning method used in recognising relationships among variables in databases and extracted in the form of rules.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AssociationRuleLearning</identifier>
						<label xml:lang="en">Association Rule Learning</label>
						<alt_label xml:lang="en">ANN</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Bayesian">
				<xs:annotation>
					<xs:documentation xml:lang="en">A method in probability and statistics based on Bayes' theorem, mainly related to statistical inference.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Bayesian</identifier>
						<label xml:lang="en">Bayesian</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ClusteringMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any method used in clustering or cluster analysis, i.e. in grouping a set of objects in such a way that objects in the same group (cluster) are more similar (in some sense or another) to each other than to those in other groups (clusters).</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ClusteringMethod</identifier>
						<label xml:lang="en">Clustering Method</label>
						<example xml:lang="en">k-means, k-nearest neighbours</example>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DecisionTrees">
				<xs:annotation>
					<xs:documentation xml:lang="en">A non-parametric supervised learning method used for classification and regression. The goal is to create a tree-like graph or model of decisions and their possible consequences by learning simple decision rules inferred from the data features.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DecisionTrees</identifier>
						<label xml:lang="en">Decision Trees</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DeepLearning">
				<xs:annotation>
					<xs:documentation xml:lang="en">A branch of machine learning based on deep neural networks. A deep neural network (DNN) is an artificial neural network (ANN) with multiple hidden layers of units between the input and output layers.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DeepLearning</identifier>
						<label xml:lang="en">Deep Learning</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DimensionalityReduction">
				<xs:annotation>
					<xs:documentation xml:lang="en">A method based on reducing the number of random variables under consideration, via obtaining a set of principal variables.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DimensionalityReduction</identifier>
						<label xml:lang="en">Dimensionality Reduction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EnsembleMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any method that uses multiple learning algorithms in an attempt to improve predictive performance not obtainable otherwise with any of the constituent learning algorithms.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/EnsembleMethod</identifier>
						<label xml:lang="en">Ensemble Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InstanceBasedLearning">
				<xs:annotation>
					<xs:documentation xml:lang="en">A family of learning algorithms that, instead of performing explicit generalization, compares new problem instances with instances seen in training, which have been stored in memory.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/InstanceBasedLearning</identifier>
						<label xml:lang="en">Instance-based Learning</label>
						<example xml:lang="en">Examples of instance-based learning algorithm are the k-nearest neighbor algorithm, kernel machines and RBF networks.</example>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KernelMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any method used in pattern analysis that relies on kernel functions, which enable it to operate in a high-dimensional, implicit feature space without ever computing the coordinates of the data in that space, but rather by simply computing the inner products between the images of all pairs of data in the feature space.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/KernelMethod</identifier>
						<label xml:lang="en">Kernel Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/RegressionAnalysis">
				<xs:annotation>
					<xs:documentation xml:lang="en">A statistical process for estimating the relationships among variables. It includes many techniques for modeling and analyzing several variables, when the focus is on the relationship between a dependent variable and one or more independent variables (or 'predictors').</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/RegressionAnalysis</identifier>
						<label xml:lang="en">Regression Analysis</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Regularisation">
				<xs:annotation>
					<xs:documentation xml:lang="en">A process of introducing additional information in order to solve an ill-posed problem or to prevent overfitting.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Regularisation</identifier>
						<label xml:lang="en">Regularisation</label>
						<alt_label xml:lang="en">Regularization</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MachineAndStatisticalLearningMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MachineLearningMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">A method of data analysis that automates model building. Using algorithms that iteratively learn from data, machine learning allows computers to find hidden insights without being explicitly programmed where to look.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MachineLearningMethod</identifier>
						<label xml:lang="en">Machine Learning Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/RuleBasedMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">A type of method that makes use of set(s) of rules to perform the relevant task.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/RuleBasedMethod</identifier>
						<label xml:lang="en">Rule-based Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/StatisticalLearningMethod">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process of using statistics and techniques related to statistics in order to understand and learn from your data so that you can predict its future.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/StatisticalLearningMethod</identifier>
						<label xml:lang="en">Statistical Learning Method</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TdmMethod</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
