			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CertaintyLevel">
				<xs:annotation>
					<xs:documentation xml:lang="en">Degree of certainty about the validity of what is being asserted in the text</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/CertaintyLevel</identifier>
						<label xml:lang="en">Certainty level</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Emotion">
				<xs:annotation>
					<xs:documentation xml:lang="en">An affective state of consciousness in which joy, sorrow, fear, hate, or the like, is experienced, as distinguished from cognitive and volitional states of consciousness [http://www.dictionary.com/browse/emotion]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Emotion</identifier>
						<label xml:lang="en">Emotion</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Event">
				<xs:annotation>
					<xs:documentation xml:lang="en">A thing that happens or takes place, especially one of importance [https://en.oxforddictionaries.com/definition/event]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Event</identifier>
						<label xml:lang="en">Event</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexicalSemanticRelation">
				<xs:annotation>
					<xs:documentation xml:lang="en">A relation holding between two or more words based on their meanings</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/LexicalSemanticRelation</identifier>
						<label xml:lang="en">Lexical semantic relation</label>
						<alt_label xml:lang="en">Semantic relation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/NamedEntity">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or phrase referring to an entity, identified and annotated as such with a name (label); examples include organizations, persons, places etc.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/NamedEntity</identifier>
						<label xml:lang="en">Named entity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Date">
				<xs:annotation>
					<xs:documentation xml:lang="en">A text unit that denotes a date, a specific point in time</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Date</identifier>
						<label xml:lang="en">Date</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Facility">
				<xs:annotation>
					<xs:documentation xml:lang="en">A human-made artifact in the domains of architecture and civil engineering [source: https://www.nltk.org/book/ch07.html]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Facility</identifier>
						<label xml:lang="en">Facility</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Geo-politicalEntity">
				<xs:annotation>
					<xs:documentation xml:lang="en">A geographical area associated with some sort of political structure</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Geo-politicalEntity</identifier>
						<label xml:lang="en">Geo-political entity</label>
						<alt_label xml:lang="en">GPE</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Location">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or group of words that denotes a geographical entity</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Location</identifier>
						<label xml:lang="en">Location</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Miscellaneous">
				<xs:annotation>
					<xs:documentation xml:lang="en">A general label for named entities other than the usual ones recognized by a system</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Miscellaneous</identifier>
						<label xml:lang="en">Miscellaneous</label>
						<alt_label xml:lang="en">MISC</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Organization">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or group of words that denotes an organization, such as company, association, institution etc.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Organization</identifier>
						<label xml:lang="en">Organization</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Person">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or group of words that refers to a person</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Person</identifier>
						<label xml:lang="en">Person</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpectralData">
				<xs:annotation>
					<xs:documentation xml:lang="en">Spectral data is essentially data derived by the use of spectroscopic instruments</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpectralData</identifier>
						<label xml:lang="en">Spectral data</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PersuasiveExpression">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or phrase used for persuasion purposes</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/PersuasiveExpression</identifier>
						<label xml:lang="en">Persuasive expression</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Polarity">
				<xs:annotation>
					<xs:documentation xml:lang="en">A feature that distinguishes between positive, negative or neutral; in sentiment analysis, it refers to determining whether the expressed opinion in a document, a sentence or an entity feature/aspect is positive, negative, or neutral. [adapted from Wikipedia]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Polarity</identifier>
						<label xml:lang="en">Polarity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/QuestionTopicalTarget">
				<xs:annotation>
					<xs:documentation xml:lang="en">The segment of a question that describes the entity about which the question is made</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/QuestionTopicalTarget</identifier>
						<label xml:lang="en">Question Topical Target</label>
						<alt_label xml:lang="en">QTT</alt_label>
						<alt_label xml:lang="en">Question topic</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Readability">
				<xs:annotation>
					<xs:documentation xml:lang="en">The ease with which a reader can understand a written text. [https://en.wikipedia.org/wiki/Readability]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Readability</identifier>
						<label xml:lang="en">Readability</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticClass">
				<xs:annotation>
					<xs:documentation xml:lang="en">A division of words into classes based on their common semantic features</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SemanticClass</identifier>
						<label xml:lang="en">Semantic class</label>
						<alt_label xml:lang="en">Semantic type</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticFrame">
				<xs:annotation>
					<xs:documentation xml:lang="en">A schematic representation of a situation involving various participants, props and other conceptual roles, each of which is a frame element</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SemanticFrame</identifier>
						<label xml:lang="en">Semantic frame</label>
						<alt_label xml:lang="en">Frame</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticRole">
				<xs:annotation>
					<xs:documentation xml:lang="en">A semantic role is the underlying relationship that a participant has with the main verb in a clause [http://www.glossary.sil.org/term/semantic-role]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SemanticRole</identifier>
						<label xml:lang="en">Semantic role</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Sentiment">
				<xs:annotation>
					<xs:documentation xml:lang="en">The affective state (judgement, feeling) of a person or group towards an entity or event</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Sentiment</identifier>
						<label xml:lang="en">Sentiment</label>
						<alt_label xml:lang="en">Opinion</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Subjectivity">
				<xs:annotation>
					<xs:documentation xml:lang="en">The linguistic expression of somebody’s opinions, sentiments, emotions, evaluations, beliefs, speculations (private states, i.e. states that are not open to objective observation or verification). [http://www.mavir.net/docs/JWiebe-Subjectivity-nov2010.pdf]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Subjectivity</identifier>
						<label xml:lang="en">Subjectivity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SyntacticoSemanticLink">
				<xs:annotation>
					<xs:documentation xml:lang="en">A link between the syntactic unit and the semantic unit (sense) of a word</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SyntacticoSemanticLink</identifier>
						<label xml:lang="en">Syntactico-semantic link</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TemporalExpression">
				<xs:annotation>
					<xs:documentation xml:lang="en">A linguistic expression (word, group of words, group of numbers etc.) that denotes time (a point in time, duration, frequency)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TemporalExpression</identifier>
						<label xml:lang="en">Temporal expression</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextualEntailment">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TextualEntailment</identifier>
						<label xml:lang="en">Textual entailment</label>
						<alt_label xml:lang="en">Entailment</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Topic">
				<xs:annotation>
					<xs:documentation xml:lang="en">The subject of a text or conversation, what it is about</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Topic</identifier>
						<label xml:lang="en">Topic</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ScholarlyCommunicationAnnotation</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/WordSense">
				<xs:annotation>
					<xs:documentation xml:lang="en">Corresponds to the structural part of a lexical entry that contains the relevant semantic, grammatical, and anthropological information for a lexical unit. [adapted from http://www.glossary.sil.org/term/sense]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/WordSense</identifier>
						<label xml:lang="en">Word sense</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeechAnnotationType">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeechAnnotationType</identifier>
						<label xml:lang="en">Speech annotation type</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OrthographicTranscription1">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/OrthographicTranscription1</identifier>
						<label xml:lang="en">Orthographic transcription</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PhoneticTranscription1">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/PhoneticTranscription1</identifier>
						<label xml:lang="en">Phonetic transcription</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ProsodicBoundary">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ProsodicBoundary</identifier>
						<label xml:lang="en">Prosodic boundary</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/StructuralAnnotationType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any type of annotation that pertains to the structure of a document</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</identifier>
						<label xml:lang="en">Structural annotation type</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Clause">
				<xs:annotation>
					<xs:documentation xml:lang="en">A clause is a subdivision of a sentence containing a subject (argument) and predicate. It is possible to have a word that implies or refers to a predicate rather than one explicitly stated. [Pei &amp; Gaynor 1980: 40, http://linguistics-ontology.org/gold/2010/Clause]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Clause</identifier>
						<label xml:lang="en">Clause</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MultiWordUnit">
				<xs:annotation>
					<xs:documentation xml:lang="en">A combination of words that are considered as forming one semantic unit</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MultiWordUnit</identifier>
						<label xml:lang="en">Multi-word unit</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Paragraph">
				<xs:annotation>
					<xs:documentation xml:lang="en">A division of a text, usually about a single theme, consisting of one or more sentences and marked by a new line, indentation or other conventions.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Paragraph</identifier>
						<label xml:lang="en">Paragraph</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Phrase">
				<xs:annotation>
					<xs:documentation xml:lang="en">A phrase is a syntactic structure that consists of more than one word but lacks the subject-predicate organization of a clause. [http://www.glossary.sil.org/term/phrase]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Phrase</identifier>
						<label xml:lang="en">Phrase</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Sentence">
				<xs:annotation>
					<xs:documentation xml:lang="en">A group of words, usually containing a verb, that expresses a thought in the form of a statement, question, instruction, or exclamation and starts with a capital letter when written [https://dictionary.cambridge.org/dictionary/english/sentence]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Sentence</identifier>
						<label xml:lang="en">Sentence</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Token">
				<xs:annotation>
					<xs:documentation xml:lang="en">A set of characters surrounded by spaces or punctuation marks, as well as punctuation marks themselves</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Token</identifier>
						<label xml:lang="en">Token</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Any type of annotation that pertains to the syntactic level</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</identifier>
						<label xml:lang="en">Syntactic annotation type</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Chunk">
				<xs:annotation>
					<xs:documentation xml:lang="en">Group of words that function together; a chunk normally includes a head and some consecutive (i.e. without gaps) preceding words</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Chunk</identifier>
						<label xml:lang="en">Chunk</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ConstituencyTree">
				<xs:annotation>
					<xs:documentation xml:lang="en">An ordered, rooted tree that represents the syntactic structure of a string according to a constituency grammar (= phrase structure grammars). It distinguishes between terminal and non-terminal nodes. The interior nodes are labeled by non-terminal categories of the grammar (phrases), while the leaf nodes are labeled by terminal categories (parts of speech). [adapted from https://en.wikipedia.org/wiki/Parse_tree]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ConstituencyTree</identifier>
						<label xml:lang="en">Constituency tree</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Constituent">
				<xs:annotation>
					<xs:documentation xml:lang="en">A word or group of words that function as a single unit in a syntactic structure</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Constituent</identifier>
						<label xml:lang="en">Constituent</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Dependency">
				<xs:annotation>
					<xs:documentation xml:lang="en">A type of syntactic relation that holds between linguistic units, where we try to recognise the head (governor) and its dependents</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Dependency</identifier>
						<label xml:lang="en">Dependency</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DependencyTree">
				<xs:annotation>
					<xs:documentation xml:lang="en">A tree that represents the dependency relations in a sentence, i.e. showing the governor (head) and its dependents with directed links</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DependencyTree</identifier>
						<label xml:lang="en">Dependency tree</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SubcategorizationFrame">
				<xs:annotation>
					<xs:documentation xml:lang="en">The number and types of syntactic arguments required by a certain lexical item (mainly verbs, but also nouns and adjectives)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SubcategorizationFrame</identifier>
						<label xml:lang="en">Subcategorization frame</label>
						<alt_label xml:lang="en">Subcategorisation frame</alt_label>
						<alt_label xml:lang="en">Argument structure</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SyntacticAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Term">
				<xs:annotation>
					<xs:documentation xml:lang="en">A term is a designation consisting of one or more words representing a general concept in a special language in a specific subject field [ISO 704:2009]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Term</identifier>
						<label xml:lang="en">Term</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--Definition for http://w3id.org/meta-share/omtd-share/Operation-->
	<xs:simpleType name="Operation">
		<xs:annotation>
			<xs:documentation xml:lang="en">The action that a software program performs or is meant to perform, and, in a broader sense, the application area where this software or relevant resources can be deployed</xs:documentation>
			<xs:appinfo>
				<identifier>http://w3id.org/meta-share/omtd-share/Operation</identifier>
				<label xml:lang="en">Operation</label>
				<alt_label xml:lang="en">Function</alt_label>
				<alt_label xml:lang="en">Task</alt_label>
				<subclassOf>http://www.w3.org/2002/07/owl#Thing</subclassOf>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Annotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of adding annotations (i.e. labels that act as notes, comments, explanations, etc.) to an item</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Annotation</identifier>
						<label xml:lang="en">Annotation</label>
						<alt_label xml:lang="en">Labelling</alt_label>
						<alt_label xml:lang="en">Tagging</alt_label>
						<alt_label xml:lang="en-us">Labeling [en-US]</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Operation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MultimediaAnnotation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MultimediaAnnotation</identifier>
						<label xml:lang="en">Multimedia annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeechAnnotation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</identifier>
						<label xml:lang="en">Speech annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfSpeakerTurns">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of marking locations in speech where there is a change of speakers</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnnotationOfSpeakerTurns</identifier>
						<label xml:lang="en">Annotation of speaker turns</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AudioSegmentation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of partitioning audio stream into homogeneous segments and classifying them into speech and non-speech segments</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AudioSegmentation</identifier>
						<label xml:lang="en">Audio segmentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ParalanguageAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of annotating paralinguistic features, i.e. vocal features that accompany speech and contribute to communication but are not generally considered to be part of the language system (e.g. vocal quality, loudness, and tempo); facial expressions and gestures are also considered as paralinguistic features [adapted from https://www.dictionary.com/browse/paralanguage]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ParalanguageAnnotation</identifier>
						<label xml:lang="en">Paralanguage annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ProsodicAnnotation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ProsodicAnnotation</identifier>
						<label xml:lang="en">Prosodic annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ProsodicSegmentation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/ProsodicSegmentation</identifier>
						<label xml:lang="en">Prosodic segmentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ProsodicAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SoundEventAnnotation">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SoundEventAnnotation</identifier>
						<label xml:lang="en">Sound event annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SoundToTextAlignment">
				<xs:annotation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SoundToTextAlignment</identifier>
						<label xml:lang="en">Sound-to-Text alignment</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpeakerDiarization">
				<xs:annotation>
					<xs:documentation xml:lang="en">Partitioning of audio stream into homogeneous segments according to speaker properties and acoustic conditions</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SpeakerDiarization</identifier>
						<label xml:lang="en">Speaker diarization</label>
						<alt_label xml:lang="en-uk">Speaker diarisation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SpeechAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of adding annotations (notes or comments) to a text; in TDM, the annotations refer mainly to the interpretative linguistic information grounded in a knowledge resource that is added manually or automatically to a text</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/TextAnnotation</identifier>
						<label xml:lang="en">Text annotation</label>
						<alt_label xml:lang="en">Linguistic annotation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DiscourseAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of adding annotations relevant to discourse, such as discourse structure, discourse markers etc.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</identifier>
						<label xml:lang="en">Discourse annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnaphoraAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The process/task of marking all linguistic expressions that appear in a certain text or across texts as referring to the same entity and linking it to this entity</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnaphoraAnnotation</identifier>
						<label xml:lang="en">Anaphora annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Argumentation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of marking argumentative structures, components and relations in a text</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Argumentation</identifier>
						<label xml:lang="en">Annotation of argumentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CoreferenceAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of attaching tags to a text unit and linking it to other text units that refer to the same entity in the world</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/CoreferenceAnnotation</identifier>
						<label xml:lang="en">Coreference annotation</label>
						<alt_label xml:lang="en">Coreference identification</alt_label>
						<alt_label xml:lang="en">Co-reference annotation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DiscourseModelling">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of building discourse models</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/DiscourseModelling</identifier>
						<label xml:lang="en">Discourse modelling</label>
						<alt_label xml:lang="en-us">Discourse modeling [en-US]</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Lemmatization">
				<xs:annotation>
					<xs:documentation xml:lang="en">Lemmatisation  (or lemmatization) in linguistics is the process of grouping together the inflected forms of a word so they can be analysed as a single item, identified by the word's lemma, or dictionary form. [Wikipedia]</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Lemmatization</identifier>
						<label xml:lang="en">Lemmatization</label>
						<alt_label xml:lang="en-uk">Lemmatisation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of adding annotations pertaining to the morphological level of analysis (e.g. gender, number, person etc.)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</identifier>
						<label xml:lang="en">Morphological annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfCompounds">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of marking compounds (single words composed of two or more free morphemes) and their parts</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnnotationOfCompounds</identifier>
						<label xml:lang="en">Annotation of compounds</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfDerivationalFeatures">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of adding annotations relevant to the derivational level of analysis (e.g. recognizing derivational affixes, tagging their meaning etc.)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnnotationOfDerivationalFeatures</identifier>
						<label xml:lang="en">Annotation of derivational features</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfMultiWordUnits">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of annotating multi-word units, i.e. combinations of words that are considered as one</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/AnnotationOfMultiWordUnits</identifier>
						<label xml:lang="en">Annotation of multi-word units</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/BelowPosTagging">
				<xs:annotation>
					<xs:documentation xml:lang="en">The annotation of words with morphological information besides the part of speech and dependent upon it (e.g. for nouns: gender, number and case; for verbs: tense, number, person etc.)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/BelowPosTagging</identifier>
						<label xml:lang="en">Below Part-of-Speech Tagging</label>
						<alt_label xml:lang="en">Below PoS Tagging</alt_label>
						<alt_label xml:lang="en">Annotation of morphological features</alt_label>
						<alt_label xml:lang="en">B-PoS Tagging</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SyllableSegmentation">
				<xs:annotation>
					<xs:documentation xml:lang="en">Segmentation of a word into syllables</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/SyllableSegmentation</identifier>
						<label xml:lang="en">Syllable segmentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/WordSegmentation">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of segmenting (cutting) a word into root and affixes</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/WordSegmentation</identifier>
						<label xml:lang="en">Word segmentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of adding morphosyntactic tags to words in a text, i.e. part-of-speech and, optionally, morphological features per part-of-speech.</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</identifier>
						<label xml:lang="en">Morphosyntactic tagging</label>
						<alt_label xml:lang="en">Morphosyntactic annotation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PosTagging">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of marking words with the part of speech (word category, e.g. noun, verb etc.) to which they belong</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/PosTagging</identifier>
						<label xml:lang="en">Part-of-Speech Tagging</label>
						<alt_label xml:lang="en">Grammatical annotation</alt_label>
						<alt_label xml:lang="en">PoS Tagging</alt_label>
						<alt_label xml:lang="en">Grammatical tagging</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Parsing">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of recognizing and marking the syntactic structure of a text or text segment</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Parsing</identifier>
						<label xml:lang="en">Parsing</label>
						<alt_label xml:lang="en">Syntactic analysis</alt_label>
						<alt_label xml:lang="en">Syntactic processing</alt_label>
						<alt_label xml:lang="en">Syntactic parsing</alt_label>
						<alt_label xml:lang="en">Syntactic annotation</alt_label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/LinguisticAnalysis</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Chunking">
				<xs:annotation>
					<xs:documentation xml:lang="en">The task/process of dividing a sentence into chunks (non-overlapping text segments consisting of a head and preceding function words and/or modifiers)</xs:documentation>
					<xs:appinfo>
						<identifier>http://w3id.org/meta-share/omtd-share/Chunking</identifier>
						<label xml:lang="en">Chunking</label>
