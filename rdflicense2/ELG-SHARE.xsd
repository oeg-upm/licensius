							<identifier>http://w3id.org/meta-share/meta-share/onLineHelpURL</identifier>
							<label xml:lang="en">online help URL</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="http://w3id.org/meta-share/meta-share/publication">
