							<identifier>http://w3id.org/meta-share/meta-share/guidelines</identifier>
							<label xml:lang="en">guidelines</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="http://w3id.org/meta-share/meta-share/installationManual">
