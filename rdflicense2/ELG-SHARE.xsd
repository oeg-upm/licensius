				<alt_label xml:lang="en">is archived by</alt_label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!--Definition for ms:isCitedBy-->
	<xs:element name="isCitedBy" type="ms:GenericDocument">
