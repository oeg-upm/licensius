<http://localhost/policy/license15euro>
        a                odrl:Set , odrl:Policy ;
        rdfs:label        "License15euros" ;
        rdfs:comment     "15 euros have to be paid to grant access. \nUser can see the offered triples, being the object only shown upon payment."@en ;
        odrl:permission  [ a            odrl:Permission ;
                           odrl:action  odrl:reproduce ;
                           odrl:duty    [ a            odrl:Duty ;
                                          rdfs:label   "Pay"@en ;
                                          odrl:action  odrl:pay ;
                                          odrl:target  "15,00 EUR" ;
										  gr:amountOfThisGood "1" ;
										  gr:UnitOfMeasurement dcat:Dataset
                                        ]
                         ] .
