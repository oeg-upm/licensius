@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat:  <http://www.w3.org/ns/dcat#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix void:  <http://rdfs.org/ns/void#> .
@prefix ldr:   <http://purl.oclc.org/NET/ldr/ns#> .
@prefix cc:    <http://creativecommons.org/ns#> .
@prefix ldp:   <http://www.w3.org/ns/ldp#>.
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .
@prefix foaf: <http://xmlns.com/foaf/spec/>
@prefix schema: <http://schema.org/> .


<http://localhost/geo>
        a             dcat:Catalog ;
		ldp:contains <http://localhost/geo/resource/Provincia/Lleida> ;
		ldp:contains <http://localhost/geo/resource/Provincia/Madrid> ;
		ldp:contains <http://localhost/geo/resource/Provincia/Valladolid> ;
		rdfs:comment "The test dataset has been taken from the IGN Geolinkeddata. The GeoLinkedData dataset includes the Spanish Autonomous Communities, provinces and province capitals, together with the geopositions of the municipalities. Additionally, links for each geographic entity to the corresponding DBPedia page have been provided." ;
        rdfs:label    "Geographic dataset with the provinces of Spain" .


<http://localhost/geo/dataset/default>
        a             dcat:Dataset ;
        rdfs:comment  "This dataset contains all the open information about the Spanish provinces" ;
        rdfs:label    "Open Data" ;
		schema:makesOffer <http://purl.org/NET/rdflicense/cc-by4.0> ;
        dct:license   <http://purl.org/NET/rdflicense/cc-by4.0> .

<http://localhost/geo/dataset/Madrid>
        a             dcat:Dataset ;
        rdfs:comment  "This dataset contains all the information about the Municipality of Madrid" ;
        rdfs:label    "Dataset Madrid" ;
		schema:makesOffer <http://purl.org/NET/rdflicense/cc-by-nc4.0> ;
        dct:license   <http://purl.org/NET/rdflicense/cc-by-nc4.0> .