    <servlet-mapping>
        <servlet-name>licensegetlicensetext</servlet-name>
        <url-pattern>/api/license/getlicensetext</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>licenseiscompatible</servlet-name>
