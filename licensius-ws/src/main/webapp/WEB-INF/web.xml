    <servlet-mapping>
        <servlet-name>licenseisopen</servlet-name>
        <url-pattern>/api/license/isopen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>licenselist</servlet-name>
        <url-pattern>/api/license/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>licensegetlicenseinfo</servlet-name>
        <url-pattern>/api/license/getlicenseinfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>licensefindlicenseinrdf</servlet-name>
        <url-pattern>/api/license/findlicenseinrdf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>licensefindlicenseintxt</servlet-name>
        <url-pattern>/api/license/findlicenseintxt</url-pattern>
    </servlet-mapping>    
<!-- INIT deprecated -->
    <servlet-mapping>
        <servlet-name>licensegetlicense</servlet-name>
        <url-pattern>/api/license/getLicense</url-pattern>
    </servlet-mapping>
<!-- END deprecated -->
    
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/api/*</url-pattern>
    </servlet-mapping>
    
    
