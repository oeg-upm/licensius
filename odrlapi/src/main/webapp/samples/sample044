#invalid. (modified example 20 in the IM. rightOperandReference to a literal)
<http://odrlapi.appspot.com/samples/sample044>
    dct:source "victor" ;
    a odrl:Offer ;
    odrl:profile <http://example.com/odrl:profile:07> ;
    odrl:permission [
        odrl:target <http://example.com/game:9090> ;
        odrl:assigner <http://example.com/org:xyz> ;
        odrl:action odrl:play ;
        odrl:constraint [
            odrl:leftOperand odrl:dateTime ;
            odrl:operator odrl:lteq;
            odrl:rightOperandReference "asdf" 
        ]            
    ] .


