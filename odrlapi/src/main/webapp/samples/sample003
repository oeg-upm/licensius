@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dct: <http://purl.org/dc/terms/> .

#valid. with metadata.
<http://odrlapi.appspot.com/samples/sample003>
    a odrl:Policy;
    dct:source "victor" ;
    odrl:permission [
        a odrl:Permission ;
        odrl:target <http://example.com/asset:9898> ;
        odrl:action odrl:reproduce
    ] .