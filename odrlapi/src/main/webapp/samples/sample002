@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
<http://odrlapi.appspot.com/samples/sample002>
    a odrl:Policy;
    odrl:permission [
        a odrl:Permission ;
        odrl:target <http://example.com/asset:9898> ;
        odrl:action odrl:reproduce
    ] ;
    odrl:prohibition [
        a odrl:Prohibition ;
        odrl:target <http://example.com/asset:9898> ;
        odrl:action odrl:modify
    ] .