@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dct: <http://purl.org/dc/terms/> .

#invalid. due to missing assignee in Prohibition ALSO FOR OTHER REASONS IN NEW SPEC.
<http://odrlapi.appspot.com/samples/sample019>
    dct:source <https://github.com/simonstey/ODRL-SHACL-Shapes/wiki/3.1.4-Agreement> ;
    a odrl:Agreement ;   
    odrl:assigner _:Bob ; 
    odrl:permission [
        a odrl:Permission ;
        odrl:target <http://example.com/asset:9898> ;
        odrl:action odrl:reproduce ;
        odrl:assignee _:Alice ;
    ] ;
    odrl:prohibition [
        a odrl:Prohibition ;
        odrl:target <http://example.com/asset:9898> ;
        odrl:action odrl:modify ;
    ] .
